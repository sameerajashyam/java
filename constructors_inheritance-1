class c
{
    public static void main(String[] args)
    {
        B s1=new B(50);
        System.out.println(s1.a+" "+s1.b);
    }
}
class A
{
    int a;
    A()
    {
        a=10;
        System.out.println("in class A constructor");
    }
    A(int val)
    {
    a=val;
    }
}
class B extends A
{
    int b;
    B()
    {
        b=20;
        System.out.println("In class B constructor");
    }
    B(int val)
    {
    b=val;
    System.out.println("a is"+a+"b is"+b);
    }
}
