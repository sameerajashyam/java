import java.util.Scanner;
class main{
public static void main(String[] args)
{
Scanner input=new Scanner(System.in);
System.out.println("enter the string:");
String s1=input.nextLine();
int len=s1.length();
int count=0;
System.out.println(len);
for (int i=0;i<len;i++)
{
if (s1.charAt(i)=='\s')
count++;

}
System.out.println(count);
}
}
